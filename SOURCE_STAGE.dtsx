              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    codart,
    codper,
    numdoc,
    tipmov,
    fecdoc,
    (YEAR(fecdoc) * 10000 + MONTH(fecdoc) * 100 + DAY(fecdoc)) AS 'SK_FECHA',
    hordoc,
    if(hordoc = '', -1 , CAST(substr(hordoc, 1, 2) AS UNSIGNED) * 10000 + CAST(substr(hordoc, 4, 2) AS UNSIGNED) * 100 + CAST(substr(hordoc, 7, 2) AS UNSIGNED)) AS 'SK_HORA',
    preart,
    totart,
    canart
FROM movdet
WHERE
      fecdoc &lt;&gt; '0000-00-00'
ORDER BY codart</property>
